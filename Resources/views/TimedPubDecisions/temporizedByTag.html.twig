{% extends 'PumukitWebTVBundle::layout.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col-xs-12">
            <div class="row">
                <div class="col-xs-12">
                    <h1 class="titulo_widget">{{ title }}</h1>
                </div>
            </div>
            {% for key, value in multimediaObjects | reverse %}
                {% include 'PumukitWebTVBundle:ByTag:list.html.twig' with {'objects' : value } %}
            {% endfor %}
        </div>
    </div>
{% endblock %}